// -*- mode: javascript -*-
{
    "Name": "Solid Mechanics ",
    "ShortName":"Solid",
    "Model":"Elasticity",
    "Materials":
    {
        "omega":{
            "name":"copper",
            "E":"124e3",
            "nu":"0.33",
            "rho":"8920e-9"
        }
    },
    "Parameters":
    {
        "rotation":
        {
            "value":"0.3" 	//pi/10=18 degree
        },
		"A":
		{
			"value":"-3.8"
		},
		"B":
		{
			"value":"0.17"
		}
    },
    "BoundaryConditions":
    {
        "displacement_x":
        {
            "Dirichlet":
            {
                "load":
                {
                    "expr":"A + (x - A)*cos(rotation) - (y - B)*sin(rotation) - x :x:y:A:B:rotation"
                }
            }
        },
        "displacement_y":
        {
            "Dirichlet":
            {
                "load":
                {
                    "expr":"B + (x - A)*sin(rotation) + (y - B)*cos(rotation) - y :x:y:A:B:rotation"
                }
            }
        },
		"displacement_z":
        {
            "Dirichlet":
            {
                "load":
                {
                    "expr":"0"
                }
            }
        },
        //"displacement_y":
        //{
         //   "Dirichlet":
         //   {
          //      "fixed":
           //     {
          //          "expr":"0"
           //     }
      //      }
       // },
        "displacement_z":
        {
            "Dirichlet":
            {
                "fixed":
                {
                    "expr":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["displacement","pressure","pid","Von-Mises"],
        "Measures":
        {
            "VolumeVariation":""
        }

    }
}
